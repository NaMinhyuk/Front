<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/search_image"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/camara"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="7dp"
        android:src="@drawable/detail_location"
        app:layout_constraintStart_toEndOf="@+id/search_image"
        app:layout_constraintTop_toTopOf="@+id/search_image" />

    <TextView
        android:id="@+id/search_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:text="울산 무거동"
        android:textColor="@color/Gray500"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <RatingBar
        android:id="@+id/search_rating"
        style="@style/Widget.AppCompat.RatingBar.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:numStars="5"
        android:progressBackgroundTint="@color/Gray300"
        android:progressTint="@color/Blue300"
        android:stepSize="0.5"
        app:layout_constraintStart_toEndOf="@+id/search_location"
        app:layout_constraintTop_toTopOf="@+id/search_location" />

    <TextView
        android:id="@+id/search_reviewCount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:text="(99 +)"
        android:textColor="@color/Gray400"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/search_rating"
        app:layout_constraintStart_toEndOf="@+id/search_rating"
        app:layout_constraintTop_toTopOf="@+id/search_rating" />

    <TextView
        android:id="@+id/search_product_name"
        android:layout_width="200dp"
        android:layout_height="20dp"
        android:layout_marginTop="10dp"
        android:text="Canon [제품 포함] EOS ..."
        android:textColor="@color/Gray900"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="7dp"
        android:text="일"
        android:textColor="@color/Gray900"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/search_image"
        app:layout_constraintStart_toStartOf="@+id/search_product_name" />

    <TextView
        android:id="@+id/search_day_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="10,000"
        android:textColor="@color/Blue300"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView32"
        app:layout_constraintStart_toEndOf="@+id/textView32"
        app:layout_constraintTop_toTopOf="@+id/textView32" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="원"
        android:textColor="@color/Gray900"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/search_day_price"
        app:layout_constraintStart_toEndOf="@+id/search_day_price"
        app:layout_constraintTop_toTopOf="@+id/search_day_price" />

    <View
        android:id="@+id/view6"
        android:layout_width="1dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:background="@color/Gray300"
        app:layout_constraintBottom_toBottomOf="@+id/textView34"
        app:layout_constraintStart_toEndOf="@+id/textView34"
        app:layout_constraintTop_toTopOf="@+id/textView34" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="시간"
        android:textColor="@color/Gray900"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/view6"
        app:layout_constraintStart_toStartOf="@+id/view6"
        app:layout_constraintTop_toTopOf="@+id/view6" />

    <TextView
        android:id="@+id/search_hour_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="5,000"
        android:textColor="@color/Blue300"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView35"
        app:layout_constraintStart_toEndOf="@+id/textView35"
        app:layout_constraintTop_toTopOf="@+id/textView35" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="원"
        android:textColor="@color/Gray900"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/search_hour_price"
        app:layout_constraintStart_toEndOf="@+id/search_hour_price"
        app:layout_constraintTop_toTopOf="@+id/search_hour_price" />

    <ImageView
        android:id="@+id/search_heart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/search_image"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/search_product_name"
        app:layout_constraintTop_toTopOf="@+id/search_image"
        app:srcCompat="@drawable/detail_heart" />

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@color/Gray300"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/search_image" />
</androidx.constraintlayout.widget.ConstraintLayout>