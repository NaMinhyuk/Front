<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/useage_header"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <ImageView
            android:id="@+id/useage_backbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/pay_backbtn"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="16dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이용내역"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textSize="16sp"
            android:textColor="@color/gray_800"/>

        <ImageView
            android:id="@+id/useage_searchbtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/regist_search_btn"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginRight="16dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/useage_top_cl"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/useage_header"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="총"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:textSize="18dp"
            android:textColor="@color/gray_800"
            android:layout_marginLeft="16dp"/>

        <TextView
            android:id="@+id/useage_count"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"
            android:textSize="18sp"
            android:textColor="#1277ED"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="40dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="건"
            android:textSize="18sp"
            android:textColor="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/useage_count"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="필터"
            android:textSize="12sp"
            android:textColor="@color/gray_800"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="38dp"/>


    </androidx.constraintlayout.widget.ConstraintLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/useage_top_cl"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/useage_body_cl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/useage_top_cl"
            app:layout_constraintStart_toStartOf="parent">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img1"
                android:id="@+id/useage_img1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/useage_img1_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img1"
                app:layout_constraintEnd_toEndOf="@id/useage_img1"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>


            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img2"
                android:id="@+id/useage_img2"
                app:layout_constraintTop_toBottomOf="@id/useage_img1"
                app:layout_constraintStart_toStartOf="parent"/>

            <TextView
                android:id="@+id/useage_img2_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img2"
                app:layout_constraintEnd_toEndOf="@id/useage_img2"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img2"
                android:id="@+id/useage_img3"
                app:layout_constraintTop_toBottomOf="@id/useage_img2"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/useage_img3_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img3"
                app:layout_constraintEnd_toEndOf="@id/useage_img3"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img1"
                android:id="@+id/useage_img4"
                app:layout_constraintTop_toBottomOf="@id/useage_img3"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/useage_img4_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img4"
                app:layout_constraintEnd_toEndOf="@id/useage_img4"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img1"
                android:id="@+id/useage_img5"
                app:layout_constraintTop_toBottomOf="@id/useage_img4"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/useage_img5_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img5"
                app:layout_constraintEnd_toEndOf="@id/useage_img5"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img1"
                android:id="@+id/useage_img6"
                app:layout_constraintTop_toBottomOf="@id/useage_img5"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/useage_img6_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img6"
                app:layout_constraintEnd_toEndOf="@id/useage_img6"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/useage_img2"
                android:id="@+id/useage_img7"
                app:layout_constraintTop_toBottomOf="@id/useage_img6"
                app:layout_constraintStart_toStartOf="parent"/>
            <TextView
                android:id="@+id/useage_img7_review"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="리뷰작성하기 >"
                android:textSize="11sp"
                android:textColor="@color/gray_600"
                app:layout_constraintTop_toTopOf="@id/useage_img7"
                app:layout_constraintEnd_toEndOf="@id/useage_img7"
                android:layout_marginTop="16dp"
                android:layout_marginRight="16dp"/>




        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>








</androidx.constraintlayout.widget.ConstraintLayout>